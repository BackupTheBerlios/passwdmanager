<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.passwdmanager"
      android:versionCode="4"
      android:versionName="1.5.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".PasswdManager"
                  android:label="@string/app_name"
                  android:configChanges="keyboard|keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
		<activity android:name=".CreateAccount"
                  android:configChanges="keyboard|keyboardHidden|orientation"></activity>
		<activity android:name=".MainMenu"
                  android:configChanges="keyboard|keyboardHidden|orientation"></activity>
		<activity android:name=".PasswdList"
                  android:configChanges="keyboard|keyboardHidden|orientation"></activity>
		<activity android:name=".ExternalList"
                  android:configChanges="keyboard|keyboardHidden|orientation"></activity>
    </application>
    <uses-sdk android:minSdkVersion="2" />
    <uses-sdk android:targetSdkVersion= "2"/>

<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
<uses-permission android:name="android.permission.WRITE_SETTINGS"></uses-permission>
</manifest> 